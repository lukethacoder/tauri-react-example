{
  "package": {
    "productName": "tauri-react-example",
    "version": "0.1.0"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:5173/",
    "beforeDevCommand": "yarn start",
    "beforeBuildCommand": "yarn build"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "identifier": "com.tauri.react.example.dev",
      "icon": [
        "../.icons/32x32.png",
        "../.icons/128x128.png",
        "../.icons/128x128@2x.png",
        "../.icons/icon.icns",
        "../.icons/icon.ico"
      ]
    },
    "allowlist": {
      "all": true
    },
    "windows": [
      {
        "title": "tauri react example",
        "width": 800,
        "height": 600,
        "resizable": true,
        "transparent": true,
        "fullscreen": false,
        "fileDropEnabled": false
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: asset: customprotocol: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  }
}
